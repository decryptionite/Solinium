        move(workspace.Terrain._Game.Workspace["Obby Box"]["Left Wall"], CFrame.new(-32.0650024,16.5000019,0.243000031,0.258819073,0,0.965925813,0,1,0,-0.965925813,0,0.258819073))
    else
