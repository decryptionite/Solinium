    pcall(function()
        if not isfile("Solinium-V2/hasviewedchanges.txt") or readfile("Solinium-V2/hasviewedchanges.txt") ~= changelogversion then
            createKohlsUi(changelogs)
            writefile("Solinium-V2/hasviewedchanges.txt", changelogversion)
        end
    end)
end)()

